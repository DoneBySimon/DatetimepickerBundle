{% block form_stylesheet %}
    {% spaceless %}
        {% for child in form %}
            {{ form_stylesheet(child) }}
        {% endfor %}
    {% endspaceless %}
{% endblock form_stylesheet %}

{% block field_stylesheet "" %}

{% block button_stylesheet "" %}


{% block dbs_datetime_stylesheet %}
    {% spaceless %}
        {% stylesheets '@bootstrap_datetimepicker_css' %}
        <link href="{{ asset_url }}" rel="stylesheet" media="screen">
        {% endstylesheets %}
    {% endspaceless %}
{% endblock dbs_datetime_stylesheet %}
